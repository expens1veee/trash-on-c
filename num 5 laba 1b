// миниммальное число в последовательности
#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    int a,min = 100000000;
    setlocale(LC_ALL, "Russian");
    do {
        cin >> a;
        if (a < min and a != 777){
            min = a;
        }
        
    }
    while (a !=777 );
    cout << min << "\n'";
return 0;
}
