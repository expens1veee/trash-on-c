// больше ли число верней границы
#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    setlocale(LC_ALL, "Russian");
    int a,b,c;
    cout << " введите числа" << "\n";
    cin >> a >> b >> c;
    if ((a >= b and a < c) or (a <= b and c > b))
        cout << "больше" << "\n";
    else if ((a >=b and a == c) or (b >= a and c == b)) cout << "равно верхней границе" << "\n";
    else cout << "меньше" << "\n";

    
return 0;
}
